@model List<Clients>

<div class="container">
    <h3>Lista klientów</h3>
    <br />
    <table class="table">
        <thead>
            <tr>
                <th>ClientId</th>
                <th>Imię Nazwisko</th>
                <th>E-mail</th>
                <th>Adres</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var c in Model)
            {
                <tr>
                    <td>@c.ClientId</td>
                    <td>@c.FirstName @c.LastName</td>
                    <td>@c.EMail</td>
                    <td>@c.Street @c.BuildingNumber/@c.FlatNumber<br />@c.City, @c.Country</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="d-flex justify-content-center">
        <button class="btn btn-info" onclick="location.href='/Management/Index'">Wróć do panelu zarządzania</button>
    </div>
</div>





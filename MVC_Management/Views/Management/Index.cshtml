<div class="row mt-3">
    <div class="col-3">
        <h4>Zarządzanie</h4>
        <button class="btn btn-info btn-list" style="width:100%;" id="btn-products">Zobacz wszystkie produkty</button>
        <button class="btn btn-info btn-list" style="width:100%;" id="btn-clients">Zobacz wszystkich klientów</button>
        <button class="mt-3 btn btn-info btn-list" style="width:100%;" id="btn-store">Zarządzaj magazynem</button>
    </div>
    <div class="col-9" id="content"></div>

    <script type="text/javascript">
        $("#btn-products").click(function () {
            $("#content").html("");
            $.get("SeeAllProducts", function (data) { $("#content").html(data); })
        });
        $("#btn-clients").click(function () {
            $("#content").html("");
            $.get("SeeAllClients", function (data) { $("#content").html(data); })
        });
        $("#btn-store").click(function () {
            $("#content").html("");
            $.get("Store", function (data) { $("#content").html(data); })
        });
    </script>



